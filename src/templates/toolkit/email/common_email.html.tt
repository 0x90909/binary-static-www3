      }
      .rightColumnContent {
          color: #777777;
          font-family: Arial;
          font-size: 11px;
          line-height: 100%;
          padding-top: 0;
          padding-right: 10px;
          padding-bottom: 20px;
          padding-left: 10px;
          text-align: left;
      }
      .rightColumnContent-footer {
          color: #777777;
          font-family: verdana;
          font-size: 10px;
          line-height: 150%;
          padding-top: 0;
          padding-right: 10px;
          padding-bottom: 5px;
          padding-left: 10px;
          text-align: center;
      }
      .rightColumnContent-footer a {
          text-decoration: none;
      }
      .rightColumnContent a:link,
      .rightColumnContent a:visited,
      /* Yahoo! Mail Override */
      .rightColumnContent a .yshortcuts
      /* Yahoo! Mail Override */
      {
          color: #288942;
          font-weight: normal;
      }
      .leftColumnContent img,
      .rightColumnContent img,
      .centerColumnContent img {
          display: block;
          height: auto;
          max-width: 260px;
          margin-right: auto;
          margin-left: auto;
      }
      #templateFooter {
          background-color: #ffffff;
          border-top: 1px solid #FFFFFF;
      }
      .footer-border {
          width: 7px;
      }
      .learnMore {
          font-weight: bold;
          color: #999999;
          font-size: 16px;
          font-family: arial;
          padding-top: 30px;
          margin-bottom: 20px;
      }
      .login {
          color: #090b4c!important;
          font-size: 18px;
          font-weight: bold;
          line-height: normal;
          text-align: center;
      }
      .content-block img {
          max-width: 200px;
          text-align: center;
          padding: 0 20px;
      }
      .content-block p {
          width: 90%;
      }
      .content-block .tickTrade {
          max-width: 125px!important;
      }
      .fund-dec {
          width: 420px;
      }
      .fund-dec p {
          line-height: normal;
      }
      /* /\/\/\/\/\/\/\/\/ MOBILE STYLES /\/\/\/\/\/\/\/\/ */
      @media only screen and (max-width: 480px) {
          /* /\/\/\/\/\/\/ CLIENT-SPECIFIC MOBILE STYLES /\/\/\/\/\/\/ */
          body,
          table,
          td,
          p,
          a,
          li,
          blockquote {
              -webkit-text-size-adjust: none !important;
          }
          /* Prevent Webkit platforms from changing default text sizes */
          body {
              width: 100% !important;
              min-width: 100% !important;
          }
          /* Prevent iOS Mail from adding padding to the body */
          /* /\/\/\/\/\/\/ MOBILE RESET STYLES /\/\/\/\/\/\/ */
          #templateBody img {
              width: 100%!important;
          }
          #bodyCell {
              padding: 5px !important;
          }
          p {
              font-size: 14px!important;
              margin-top: 20px!important;
              margin-bottom: 20px!important;
          }
          /* /\/\/\/\/\/\/ MOBILE TEMPLATE STYLES /\/\/\/\/\/\/ */
          /* ======== Page Styles ======== */
          #templateContainer {
              max-width: 700px !important;
              width: 100% !important;
          }
          h1 {
              font-size: 26px !important;
              line-height: 100% !important;
          }
          h2 {
              font-size: 20px !important;
              line-height: 100% !important;
          }
          h3 {
              font-size: 18px !important;
              line-height: 100% !important;
          }
          h4 {
              font-size: 16px !important;
              line-height: 100% !important;
          }
          /* ======== Header Styles ======== */
          #templatePreheader {
              display: none !important;
          }
          #headerImage {
              height: auto !important;
              max-width: 700px !important;
              margin-bottom: 20px!important;
              /*width: 100% !important;*/
          }
          .headerContent {
              font-size: 20px !important;
              line-height: 125% !important;
          }
          /* ======== Body Styles ======== */
          .bodyContent {
              font-size: 14px !important;
              line-height: 125% !important;
              padding-left: 10px!important;
              padding-right: 10px!important;
          }
          .content-block {
              width: 100%!!important;
              display: block!important;
              text-align: center!important;
              padding: 0 20px!important;
          }
          .content-block p {
              width: 100%!important;
          }
          /* ======== Column Styles ======== */
          .templateColumnContainer {
              display: block !important;
              width: 100% !important;
          }
          .mainColumnContainer {
              display: block !important;
              width: 100% !important;
          }
          .calltoaction {
              text-align: center!important;
              margin-top: 20px!important;
              margin-bottom: 20px!important;
          }
          .button {
              margin-top: 15px!important;
              padding: 13px!important;
              color: #ffffff!important;
              font-weight: bold!important;
              font-size: 15px!important;
              display: inline!important;
              background: #288942!important;
              text-decoration: none!important;
              text-align: center!!important;
          }
          .buttonSmall {
              margin-top: 15px!important;
              ;
              padding: 10px!important;
              ;
              color: #ffffff!important;
              font-weight: bold!important;
              ;
              font-size: 12px!important;
              ;
              display: inline!important;
              background: #288942!important;
              ;
              text-decoration: none!important;
              text-align: center!important;
          }
          .leftM {
              margin-left: 0px!important;
          }
          .fund-dec {
              width: 100%!important;
              display: block!important;
          }
          .mobile-hide {
              display: none!important;
          }
          .columnImage {
              height: auto !important;
              max-width: 480px !important;
              /*width: 100% !important;*/
          }
          .leftColumnContent {
              font-size: 12px !important;
              line-height: 125% !important;
              text-align: justify;
          }
          .centerColumnContent {
              font-size: 12px !important;
              line-height: 125% !important;
              text-align: justify;
          }
          .rightColumnContent {
              font-size: 12px !important;
              line-height: 125% !important;
              text-align: justify;
          }
          /* ======== Footer Styles ======== */
          .centerColumnContent-footer {
              font-size: 12px!important;
          }
          .leftColumnContent-footer {
              font-size: 12px!important;
          }
          .templateColumnContainer {
              border-bottom: 1px dotted #777777!important;
          }
          .footer-border {
              display: none!important
          }
          /* Place footer social and utility links on their own lines, for easier access */
      }
  </style>
</head>
<body>
