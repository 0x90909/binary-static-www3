          margin-bottom: 1em;
      }
      .list {
          color: #333333;
      }
      .list li {
          margin-bottom: 10px;
      }
      .bodyContent a:link,
      .bodyContent a:visited,
      /* Yahoo! Mail Override */
      .bodyContent a .yshortcuts
      /* Yahoo! Mail Override */
      {
          color: #288942;
          text-decoration: none;
      }
      .bodyContent img {
          display: inline;
          height: auto;
          max-width: 560px;
      }
      .templateColumnContainer {
          width: 200px;
      }
      #templateColumns {
          background-color: #ffffff;
          border-top: 1px solid #FFFFFF;
          border-bottom: 1px solid #f0f0f0;
      }
      .calltoaction {
          text-align: center!important;
          margin-top: 20px;
          margin-bottom: 20px;
      }
      .button {
