- stash(
-   title => l('Partners'),
- );
.grd-container.grd-default-container
  .grd-grid-12.static_full
    %h1= stash->{title}
    %p= l('[_1] provides business partnership serivces via various programs.', stash->{website_name})
  .grd-grid-12.grd-grid-mobile-12.grd-parent
    .grd-grid-4.grd-grid-mobile-12.grd-parent
      .grd-grid-6.grd-with-padding
        %a.pjaxload{:href => url_for('/affiliate/signup')}
          %img#charting_icon.responsive.charting-page-icon{:src => url_for('images/pages/partners/affiliate-program.svg')}
      .grd-grid-12
        %h4
          %a.pjaxload{:href => url_for('/affiliate/signup')}= l('Affiliate Program')
