          %img#charting_icon.responsive.charting-page-icon{:src => url_for('images/pages/partners/api.svg')}
      .grd-grid-12
        %h4
          %a.pjaxload{:href => url_for('/partnerapi')}= l('Partner API')
        %p= l('[_1] provides business partners with two application programming interfaces (APIs).', stash->{website_name})
    .grd-grid-4.grd-grid-mobile-12.grd-parent
      .grd-grid-6.grd-with-padding
        %a.pjaxload{:href => url_for('/open-source-projects')}
          %img#charting_icon.responsive.charting-page-icon{:src => url_for('images/pages/partners/open-source-venture.svg')}
